<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sitbbsr.sitcinemax">


    <queries>
        <intent>
            <action android:name="com.google.android.youtube.api.service.START" />
        </intent>
    </queries>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SITCinemax">
        <activity
            android:name="com.sitbbsr.sitcinemax.UserPollSelection"
            android:exported="false" />
        <activity
            android:name="com.sitbbsr.sitcinemax.SingleTicket_QR"
            android:exported="false" />
        <activity
            android:name="com.sitbbsr.sitcinemax.FeedbackUser"
            android:exported="false" />
        <activity
            android:name="com.sitbbsr.sitcinemax.UserBookedTickets"
            android:exported="false" />
        <activity
            android:name="com.sitbbsr.sitcinemax.BookingConfirmation"
            android:exported="false" />
        <activity
            android:name="com.sitbbsr.sitcinemax.AboutMovie"
            android:exported="false" />
        <activity
            android:name="com.sitbbsr.sitcinemax.ChooseSeatLayout"
            android:exported="false" />
        <activity
            android:name="com.sitbbsr.sitcinemax.BookTickets"
            android:exported="false" />
        <activity
            android:name="com.sitbbsr.sitcinemax.UserBookTickets"
            android:exported="false" />
        <activity
            android:name="com.sitbbsr.sitcinemax.EditUserProfile"
            android:exported="false" />
        <activity
            android:name="com.sitbbsr.sitcinemax.AboutSITCinemax"
            android:exported="false" />
        <activity
            android:name="com.sitbbsr.sitcinemax.ContactUs"
            android:exported="false" />
        <activity
            android:name="com.sitbbsr.sitcinemax.UserLogin"
            android:exported="false" />
        <activity
            android:name="com.sitbbsr.sitcinemax.UserPhoneNumberVerification"
            android:exported="false" />
        <activity
            android:name="com.sitbbsr.sitcinemax.UserDashBoard"
            android:exported="false" />
        <activity
            android:name="com.sitbbsr.sitcinemax.UserQrCode"
            android:exported="false" />
        <activity
            android:name="com.sitbbsr.sitcinemax.UserToDoList"
            android:exported="false" />
        <activity
            android:name="com.sitbbsr.sitcinemax.UserSignUp"
            android:exported="false" />
        <activity
            android:name="com.sitbbsr.sitcinemax.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name="com.sitbbsr.sitcinemax.PushNotificationService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>